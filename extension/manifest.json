{
    "name": "Clickfresh",
    "description": "desc",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "./background.js"
    },
    "action": {
        "default_popup": "./popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "scripting",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}